<%= simple_form_for(@hypothesis) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :content %>

    <%= f.input :experiment_id, collection: Experiment.all, label_method: :title %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
