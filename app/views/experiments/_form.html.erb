<%= simple_form_for(@experiment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :title %>

    <%= f.input :tagline %>

    <%= f.input :description %>

    <%= f.input :purpose %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
