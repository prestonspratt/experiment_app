<h1>Listing researchsections</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Videolink</th>
    <th>Experiment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @researchsections.each do |researchsection| %>
  <tr>
    <td><%= researchsection.content %></td>
    <td><%= researchsection.videolink %></td>
    <td><%= researchsection.experiment_id %></td>
    <td><%= link_to 'Show', researchsection %></td>
    <td><%= link_to 'Edit', edit_researchsection_path(researchsection) %></td>
    <td><%= link_to 'Destroy', researchsection, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

